{
  "version": "1.6.2",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "noExternalResolve": false,
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "experimentalDecorators": true,
    "outDir": ".tmp",
    "sourceMap": true
  },
  "filesGlob": [
    "src/app/*/*.ts"
  ],
  "files": [
    "src/app.ts",
    "src/constants.ts"
  ]
}
